    <script>
      $(document).ready(function () {
        $.validator.addMethod(
          "agent_c",
          function (value, element) {
            var result = false;
            $.ajax({
              url: "/admin/checkedUnicUser",
              type: "POST",
              data: {
                agent_c: value,
              },
              async: false,
              success: function (data) {
                if (data == "true") {
                  result = true;
                }
              },
            });
            return result;
          },
          "User ID is already taken"
        );

        $("#myForm").validate({
          rules: {
            agent_c: {
              required: true,
              agent_c: true,
            },
            name: "required",
            mobile: "required",
            max_amt: {
              required: true,
              digits: true,
            },
            allow_collection_days: {
              required: true,
              digits: true,
            },
            pin: {
              required: true,
              minlength: 4,
              maxlength: 4,
              digits: true,
            },
            cid: {
              minlength: 4,
              maxlength: 4,
              required: true,
              equalTo: "#pin",
            },
            deviceId: "required",
            email: {
              required: true,
              email: true,
            },
          },
          messages: {
            agent_c: "Please enter Agent Code",
            name: "Please enter name",
            mobile: "Please enter Mobile No",
            max_amt: {
              required: "Please enter Max Allowable Collection Amount",
              digits:
                "Max Allowable Collection Amount must contain only digits",
            },
            pin: {
              required: "Please enter 4 digits pin",
              minlength: "pin must be 4 digits",
              maxlength: "pin must be 4 digits",
              digits: "pin must contain only digits",
            },
            cid: {
              required: "Please enter 4 digits pin",
              minlength: "pin must be 4 digits",
              maxlength: "pin must be 4 digits",
              digits: "pin must contain only digits",
              equalTo: "Passwords do not match",
            },
            deviceId: "Please enter Device Id",
            email: {
              required: "Please enter email",
              email: "Please enter a valid email address",
            },
          },
        });
      });
    </script>
