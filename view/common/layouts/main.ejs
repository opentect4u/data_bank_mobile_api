<% if (error.length > 0) { %>
  <script>
    toastr.error('<%= error %>')
  </script>
<% } else if (success.length > 0) { %>
  <script>
    toastr.success('<%= success %>')
  </script>
<% }else if (info.length > 0) { %>
  <script>
    toastr.info('<%= info %>')
  </script>
<% }else if (warning.length > 0) { %>
  <script>
    toastr.warning('<%= warning %>')
  </script>
<% } %>
