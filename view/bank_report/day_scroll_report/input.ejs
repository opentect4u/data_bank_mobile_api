<div class="main-panel">
  <div class="content-wrapper">
    <div class="card col-8">
      <div class="card-body">
        <div class="titleSec">
          <!-- <button type="button" class="btn btn-primary">Export as CSV</button> -->
          <h2>Day scroll report</h2>
        </div>
        <!-- <input type="file" name="myFile" class="" id="pctxFile"> -->
        <div class="row">
          <form action="/bank/day_scroll_report_post" method="post">
            <div class="col-sm-12 row">
              <div class="form-group col-6">
                <label for="from_date">Form Date:</label>
                <input type="date" name="from_date" class="form-control" value="<%=nowdate%>" required/>
              </div>

              <div class="form-group col-6">
                <label for="to_date">To Date:</label>
                <input type="date" name="to_date" class="form-control" value="<%=nowdate%>" required/>
              </div>

              <div class="form-group col-12">
                <label for="branch_code">Select Branch:</label>
                <select name="branch_code" class="form-control s2id" id="branch_code">
                  <option value="">Select Branch</option>
                  <option value="0">All Branch</option>
                  <% data.forEach(function(item) { %>
                  <option value="<%= item.branch_code %>">
                    <%= item.branch_name %>
                  </option>

                  <% }); %>
                </select>
              </div>

              <div class="form-group col-12">
                <input
                  type="submit"
                  value="SUBMIT"
                  class="btn btn-primary btn-block"
                  id="downloadButton"
                />
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
